stepback <- function(x, alpha) {
within_alpha <- x < lead(x) * (1 + alpha)
xmin <- which.min(x)
y <- xmin
keep_going <- TRUE
while(keep_going) {
if (within_alpha[y - 1]) {
y <- y - 1
keep_going <- TRUE
} else {
keep_going <- FALSE
}
}
return(y)
}
